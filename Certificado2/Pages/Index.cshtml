<script>
      $(document).ready(function(){
    $('.banner-slider').owlCarousel({
        items:1,
        loop:true,
        autoplay:true, // Activa el autoplay
        autoplayTimeout:2000, // Establece el tiempo de espera en milisegundos (en este caso, 2 segundos)
        autoplayHoverPause:true, // Pausa el autoplay cuando se pasa el mouse sobre el carrusel
